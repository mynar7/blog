{
  "name": "blog",
  "private": true,
  "description": "A blog called Strings and Things",
  "version": "0.1.0",
  "author": "Lee Warrick <leewarrick@gmail.com>",
  "homepage": "https://leewarrick.com/blog",
  "bugs": {
    "url": "https://github.com/mynar7/blog/issues"
  },
  "dependencies": {
